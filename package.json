{
  "name": "nginx-ip-whitelister",
  "version": "1.3.1",
  "description": "Authentication backend that works with the Nginx ngx_http_auth_request_module and whitelists remote IP address if a key is provided.",
  "main": "index.js",
  "type": "module",
  "exports": {
    "node": {
      "import": "./index.js"
    }
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "keywords": [
    "nginx",
    "ngx_http_auth_request_module",
    "ip",
    "address",
    "whitelist",
    "auth",
    "request"
  ],
  "author": "zuavra.net",
  "license": "MIT",
  "dependencies": {
    "dotenv": "=16.3.1",
    "jssha": "=3.3.1",
    "mmdb-lib": "=2.0.2",
    "netmask": "=2.0.2",
    "tiny-lru": "=11.2.0",
    "totp-auth": "=1.0.11"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
